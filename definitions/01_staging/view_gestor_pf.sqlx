config {
  type: "view",
  schema: "staging"
}
select
  CNPJ_FUNDO,
  DT_REG as DATA_REGISTRO,
  PF_PJ_GESTOR as PESSOA_GESTOR,
  CPF_CNPJ_GESTOR as CPF_GESTOR,
  GESTOR,
  DT_INI_GESTOR as DATA_INICIO,
  DT_FIM_GESTOR as DATA_FIM
from
  ${ref("cvm_fi_hist_gestor")}
where 
  PF_PJ_GESTOR = 'PF'
  and CNPJ_FUNDO <> 'null'
