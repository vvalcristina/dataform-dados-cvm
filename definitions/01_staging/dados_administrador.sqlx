config {
  type: "table",
  schema: "staging"
}
select 
  CNPJ_FUNDO as cnpj_fundo,
  CAST (DT_REG as string) as data_registro_fundo,
  CNPJ_ADMIN as cnpj_administrador,
  ADMIN	as administrador,
  CAST (DT_INI_ADMIN as string) as data_inicio,
  CAST (DT_FIM_ADMIN as string) as data_fim
from
  ${ref("cvm_fi_hist_admin")}
where 
  CNPJ_FUNDO <>'null'
  and DT_INI_ADMIN <>'null'